<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <servlet>
        <servlet-name>LastFmArtistsServlet</servlet-name>
        <servlet-class>com.lastfm.view.LastFmArtistsServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LastFmArtistsServlet</servlet-name>
        <url-pattern>/artists</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LastFmTopTracksServlet</servlet-name>
        <servlet-class>com.lastfm.view.LastFmTopTracksServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LastFmTopTracksServlet</servlet-name>
        <url-pattern>/toptracks</url-pattern>
    </servlet-mapping>
</web-app>
